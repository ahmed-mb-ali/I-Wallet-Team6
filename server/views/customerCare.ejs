
<%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %>




    <!-- Main Content -->
    <main>
      <main class="container">
        <div class="row">
           <div class="col">
              <div class="jumbotron">
                 <h1 class="display-7"><%= title %></h1>
              </div>
           </div>
        </div>
           <div class="row">
             <div class="col">
               <p class="display-10">Welcome to <%= title %> </p>
               <hr class="my-4">
             </div>
          </div>  

 <form >
     
      <div class="form-group">
        <input type="email"  id="emailAddress" class="form-control" placeholder="Email Address"> <!--Here user can input email address-->
      </div><br>
      
      <div class="form-group">
        <input type="contact_number" id="contactNumber" class="form-control" placeholder="Contact Number"> <!--Here user can input contact number-->
      </div><br>
 
      
      <div class="form-group">
        <textarea name="message" id="message"  class="form-control" placeholder="Type your message here" cols="30" rows="8"></textarea> <!--Here user can input Text Area-->
      </div><br>

      
        <div class="text-first mt-6">
                  <button id="loginButton" type="submit" class="btn btn-primary"><i class="fas fa-sign-in-alt"></i> Submit</button>
                  <button id="cancelButton" type="reset" class="btn btn-warning"><i class="fas fa-undo"></i> Cancel</button>
        </div>
    </form><br>

        </main>


  


<%- include partials/bottom_nav.ejs %>

<%- include partials/footer.ejs %>






